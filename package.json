{
  "name": "tb-icons",
  "version": "0.1.0",
  "description": "A classified icons set that consists of a part of Material Design icons and some original icons by TB-UI team.",
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run gulp:build",
    "dev": "npm run gulp:dev & npm run webpack:dev",
    "preghpages": "npm run gulp:build && npm run webpack:build",
    "ghpages": "./node_modules/.bin/gulp gh-pages",
    "gulp:build": "NODE_ENV=build ./node_modules/.bin/gulp",
    "gulp:dev": "NODE_ENV=development ./node_modules/.bin/gulp dev",
    "prewebpack:build": "rm -rf ./build",
    "webpack:build": "NODE_ENV=build ./node_modules/.bin/webpack --progress",
    "webpack:dev": "NODE_ENV=development ./node_modules/.bin/webpack-dev-server --hot --progress"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/teambition/TB-Icons.git"
  },
  "author": {
    "name": "Sune Bear",
    "email": "hi@sunebear.com",
    "url": "http://sunebear.com"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "clipboard": "^1.5.10",
    "css-loader": "^0.23.1",
    "cssnano": "^3.6.2",
    "del": "^2.2.0",
    "ejs-compiled-loader": "^2.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "gulp": "^3.9.0",
    "gulp-consolidate": "^0.2.0",
    "gulp-gh-pages": "^0.5.4",
    "gulp-iconfont": "^8.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-svg-symbols": "^1.0.0",
    "gulp-svgmin": "^1.2.2",
    "html-webpack-plugin": "^2.19.0",
    "inflection": "^1.10.0",
    "json-loader": "^0.5.4",
    "merge-stream": "^1.0.0",
    "node-dir": "^0.1.12",
    "normalize.css": "^4.1.1",
    "postcss-loader": "^0.9.1",
    "run-sequence": "^1.2.1",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.1.0",
    "svgo": "^0.6.6",
    "through2": "^2.0.1",
    "url-loader": "^0.5.7",
    "vinyl": "^1.1.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
